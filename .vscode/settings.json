{
	"editor.insertSpaces": false,
	"files.eol": "\n",
	"files.trimTrailingWhitespace": true,
	"files.exclude": {
		".git": true,
		"**/.DS_Store": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		".build/**": true,
		"out*/**": true,
		"i18n/**": true,
		"extensions/**/out/**": true
	},
	"tslint.enable": true,
	"tslint.rulesDirectory": "build/lib/tslint"
}