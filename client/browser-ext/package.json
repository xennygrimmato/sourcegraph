{
  "name": "sourcegraph-chrome-extension",
  "version": "0.0.1",
  "description": "Sourcegraph browser extension for Chrome",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha -r ./test/setup-app test/app",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha test/e2e",
    "dev": "node scripts/dev",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "build-docker": "docker build -t sourcegraph/browser-ext-build scripts/build-env",
    "dist": "npm run build-docker && docker run -v `pwd`:/browser-ext -i -t sourcegraph/browser-ext-build /bin/bash /browser-ext/scripts/build-env/dist.sh"
  },
  "author": "John Rothfels <john@sourcegraph.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "chai": "^3.2.0",
    "chromedriver": "^2.19.0",
    "co-mocha": "^1.1.2",
    "cross-env": "^1.0.7",
    "crx": "^3.0.3",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^4.5.4",
    "jade": "^1.11.0",
    "jsdom": "^8.2.0",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.0",
    "postcss-modules-values": "^1.1.2",
    "react-addons-test-utils": "^0.14.3",
    "redux-mock-store": "^1.1.1",
    "rimraf": "^2.4.3",
    "selenium-webdriver": "^2.47.0",
    "shelljs": "^0.6.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "amplitude-js": "git://github.com/amplitude/Amplitude-Javascript.git#v3.0.0",
    "bluebird": "^3.3.4",
    "btoa": "^1.1.2",
    "classnames": "^2.1.3",
    "lodash": "^4.11.2",
    "react": "^0.14.7",
    "react-css-modules": "^3.7.6",
    "react-dock": "^0.2.3",
    "react-dom": "^0.14.7",
    "react-icon-base": "^2.0.0",
    "react-icons": "^2.0.1",
    "react-redux": "^4.3.0",
    "redux": "^3.2.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "utf8": "^2.1.1"
  }
}
